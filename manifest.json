{
	"manifest_version": 2,
	"name": "Angular Weather",
	"description": "Checkin the weather maaaaaan",
	"version": "1",
	"permissions": [
	"http://api.wunderground.com/api",
	"http://autocomplete.wunderground.com/api"
	],
	"background": {
		"scripts": ["js/angular.min.js"]
	},
	"content_security_policy": "script-src 'self'; object-src 'self'",
	"chrome_url_overrides":{
		"newtab": "index.html"
	}

	}
